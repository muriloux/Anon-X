{
  "manifest_version": 3,
  "name": "Anon X",
  "description": "Become Anondraw X User",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.anondraw.com/*", "https://anondraw.com/*"],
      "js": ["bundle.min.js"]
    }
  ]
}